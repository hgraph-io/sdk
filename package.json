{
  "homepage": "https://hgraph.io",
  "name": "@hgraph.io/sdk",
  "description": "hgraph.io sdk",
  "version": "0.8.3",
  "main": "./dist/node-cjs.js",
  "module": "./dist/node-esm.js",
  "browser": {
    "./dist/node-cjs.js": "./dist/browser-cjs.js",
    "./dist/node-esm.js": "./dist/browser-esm.js"
  },
  "author": "Tyler McDonald <tyler@hgraph.io> (https://hgraph.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hgraph.io/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/hgraph.io/sdk/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "graphql",
    "nft",
    "token",
    "hedera",
    "mirror-node",
    "crypto",
    "api",
    "hgraph",
    "web3",
    "decentralized",
    "dao"
  ],
  "types": "./dist/index.d.ts",
  "dependencies": {
    "asn1.js": "^5.4.1",
    "bufferutil": "^4.0.7",
    "encoding": "^0.1.13",
    "ethers": "^6.13.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.3",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-ws": "^5.0.0",
    "jose": "^4.14.4",
    "json-bigint": "^1.0.0",
    "rfc6902": "^5.1.1",
    "ts-invariant": "^0.10.3",
    "tweetnacl": "^1.0.3",
    "utf-8-validate": "^6.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.5.0",
    "@types/json-bigint": "^1.0.1",
    "@types/node": "^24.0.12",
    "@types/react": "^18.3.2",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "0.21.4",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-jest": "^29.1.1",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "watch": "nodemon -w src -e ts,json,gql,graphql --exec 'npm run build'",
    "build": "node ./build.mjs",
    "update-schema": ". ./scripts/update-schema.sh",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "prepublishOnly": "rm -Rf dist && npm run build",
    "test": "vitest run && jest"
  }
}
